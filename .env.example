# Weatherstack API key — obtain from https://weatherstack.com/dashboard
WEATHERSTACK_API_KEY=your_api_key_here

# Azure OpenAI — obtain from your resource's "Keys and Endpoint" page
# Endpoint format: https://<resource-name>.cognitiveservices.azure.com
PROJECT_ENDPOINT="your_cognitiveservices_endpoint_here"
AZURE_AI_API_KEY="your_api_key_here"
AZURE_API_VERSION="your_api_version_here"
MODEL_DEPLOYMENT_NAME="your_deployment_name_here"

# Optional: override the MCP server port (default: 8000)
# MCP_PORT=8000

# Agent backend — base URL of the MCP server (no trailing slash)
MCP_SERVER_URL=http://localhost:8000

# Optional: override the agent backend port (default: 8001)
# AGENT_PORT=8001
